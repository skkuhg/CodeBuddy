# API Keys Configuration
# Copy this file to .env and fill in your actual API keys

# Tavily AI API Key
# Get your key from: https://app.tavily.com/
TAVILY_API_KEY=your_tavily_api_key_here

# OpenAI API Key  
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Google Cloud Vision API Key
GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Optional: OCR.space API Key
OCR_SPACE_API_KEY=your_ocr_space_api_key_here

# Optional: Azure Vision API Configuration
AZURE_VISION_API_KEY=your_azure_api_key_here
AZURE_VISION_ENDPOINT=your_azure_endpoint_here